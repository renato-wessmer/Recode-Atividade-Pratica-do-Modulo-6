<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AgenciaDeViagensNyssa</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/AgenciaDeViagensNyssa.styles.css" asp-append-version="true" />
</head>
<body>
    <!-- Cabeçalho/Barra de navegação/Navbar -->
    <header class="p-3 bg-white text-white fixed-top">
        <div class="container">
            <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
                    <a class="link-dark" href="/Index"><svg class="bi" width="50" height="50"><use xlink:href="#logo" /></svg></a>
                </a>
                <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                    <li>
                        <a href="/Index" class="nav-link px-2 text-secondary">
                            Home
                        </a>
                    </li>
                    <li>
                        <a href="/Destinos/Index" class="nav-link px-2 text-secondary">
                            Destino
                        </a>
                    </li>
                    <li>
                        <a href="/Promocoes/Index" class="nav-link px-2 text-secondary">
                            Promoções
                        </a>
                    </li>
                    <li>
                        <a href="/Contato/Index" class="nav-link px-2 text-secondary">
                            Contato
                        </a>
                    </li>
                    <li>
                        <a href="/Clients/Index" class="nav-link px-2 text-secondary">
                            Clientes
                        </a>
                    </li>
                </ul>
                <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3" role="Pesquisa">
                    <input type="search" class="form-control form-control-dark text-dark bg-transparent" placeholder="Pesquisa..." aria-label="Search">
                </form>
                <!---O atributo aria-label é usado para definir uma string na tag do elemento atual, usa-se nos casos que a tag do texto não seja visível na tela--->
                <div class="text-end">
                    <button type="button" class="btn btn-success" onclick="window.location.href ='/Login/Index'">
                        Conecte-se
                    </button>
                    <button type="button" class="btn btn-success" onclick="window.location.href ='/Inscrevase/Index'">
                        Inscrever-se
                    </button>
                </div>
            </div>
        </div>
    </header>


    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <!-- Rodapé / Footer -->
